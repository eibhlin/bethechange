      					<input type="checkbox"></td>
                    <td><%= Umowa[i].Kwota %></td>
                </tr>
                <% 
				} //for
				%>
                <tr>
                	<td>Podsumowanie</td>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                    <td><%= Umowa[i].Kwota %></td>
