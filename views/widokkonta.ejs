                    <td><%= Umowa[i].imieNazwisko %></td>
                    <td><a href="<%= Umowa[i].zalacznik %>">Pobierz załącznik</a></td>
                    <td><label value="1" class="checkbox">
      					<input type="checkbox"></td>
                    <td><%= Umowa[i].Kwota %></td>
                </tr>
                <% 
				} //for
				%>
                <tr>
                <%  for(var i=0;i<Expenses.length;i++) { %>
                	<td>2</td>
